! A simple testcase for testing CircuitsAndDynamics module.
! Author: Eelis Takala, Trafotek Oy
! Original date: October 2015
! eelis.takala@trafotek.fi

Check Keywords "Warn"
INCLUDE 1383/mesh.names

Header 1
   Mesh DB "." "1383"
   Results Directory "res"
End

$ ntimesteps = 20
$ freq = 50
$ omega = 2*pi*freq
$ periods = 1
$ stepsize = periods/(ntimesteps*freq)
Simulation 1
   Max Output Level = 3
   Coordinate System = Axi symmetric
   Simulation Type = Transient
   Timestepping Method = BDF
   BDF Order = 1
   Timestep Sizes(1) = $ stepsize
   Timestep Intervals(1) = $ ntimesteps * periods  
   Steady State Max Iterations = 1
   Mesh Levels = 3
End

!INCLUDE 1383-circuits-components.definition
INCLUDE transient_foil_circuit.definition

Solver 1  !---- DirectionSolver, DirectionSolver
   Exec Solver = Before all
   Procedure = "DirectionSolver" "DirectionSolver"
   Linear System Solver = Iterative
   Linear System Iterative Method = BiCGStab
   Linear System Max Iterations = 5000
   Linear System Convergence Tolerance = 1.0e-10
   Linear System Abort Not Converged = True
   Linear System Residual Output = 1000
   Equation = Direction
   Variable = Alpha
End

Solver 2
   Exec Solver = Always
   Equation = Circuits
   Variable = X
   No Matrix = Logical True
   Procedure = "CircuitsAndDynamics" "CircuitsAndDynamics"
End

Solver 3
   Equation = "Mag"
   Variable = A
   Procedure = "MagnetoDynamics2D" "MagnetoDynamics2D"
   NonLinear System Relaxation Factor=1
   Export Lagrange Multiplier = Logical True
   Linear System Direct Method = UMFPACK
   Linear System Solver = Iterative
   Linear System Solver = Direct 
   Linear System Iterative Method = BicgstabL
   Linear System Convergence Tolerance = 1.e-10
   Linear System Preconditioning = Circuit
   Linear System Max Iterations = 3000
   Linear System Residual Output = 10
   Linear System Abort not Converged = False
   Linear System ILU Order = Integer 0
   Linear System ILUT Tolerance = 1e-8
   Steady State Convergence Tolerance = 1e-10
End


Solver 4  !---- MagnetoAndDynamics, MagnetoDynamicsCalcFields
! Exec Solver = Never
  Equation = MGDynamicsCalc
  Procedure = "MagnetoDynamics" "MagnetoDynamicsCalcFields"
  
  Potential Variable = String "A"
  Steady State Convergence Tolerance = 1.0e-6
  
  Linear System Convergence Tolerance = 1.0e-6
  Linear System Iterative Method = CG
  Linear System Max Iterations = 5000
  Linear System Solver = Iterative
  Linear System Symmetric = True
  Linear System Residual Output = 1
  Linear System Preconditioning = ILU0
  Linear System Precondition Recompute = 1
  Linear System Abort Not Converged = False

  Discontinuous Bodies = Logical True
  
  Calculate Nodal Fields = Logical True
  Calculate Elemental Fields = Logical True
End


Solver 5
   !Exec Solver = Never
   Equation = "ResultOutput"
   Procedure = "ResultOutputSolve" "ResultOutputSolver"
   Output File Name = 1383-results
   Vtu format = Logical True
   Save Geometry Ids = Logical True
   Output format = Gmsh
End

Solver 6
   Exec Solver = Always
   Equation = Circuits Output
   Procedure = "CircuitsAndDynamics" "CircuitsOutput"
End

Solver 6
   Exec Solver = Always
   Equation = "sv"
   Procedure = "SaveData" "SaveScalars"
   Filename = dat/1383.dat
End

Equation 1
   Active Solvers(4) = 3 4 5 6
End

Equation 2
   Active Solvers(6) = 1 2 3 4 5 6
End

Material 1
   Name = iron
   Electric Conductivity = 0
   Relative Permeability = 2000
End

Material 2
   Name = air
   Electric Conductivity = 0
   Relative Permeability = 1
End

Material 3
   Name = aluminium
   Relative Permeability = 1
   Electric Conductivity = 1e7
End

Body 1
   Name = Core
   Target Bodies(6) = 1 2 3 4 5 6
   Equation = 1
   Material = 1
End

Body 2
   Name = air
   Target Bodies(2) = 9 7
   Equation = 1
   Material = 2
End

Body 3
   Name = test 1
   Target Bodies = 8
   Equation = 2
   Material = 3
End

Boundary Condition 1
   Target Boundaries(4) = $ center0 center1 center2 center3
   A = Real 0
End

Boundary Condition 2
   Target Boundaries(2) = 6 8
   Name = "Air Gap"
   Air Gap = logical True
   Air Gap Length = Real 0.0001
   Air Gap Relative Permeability = Real 1
End

Boundary Condition 3
   Target Boundaries(7) = 12 15 16 19 20 21 27
   Infinity BC = Logical True
End

Boundary Condition 4
   Target Boundaries = $ test_1_alpha0
   alpha = Real 0
End

Boundary Condition 5
   Target Boundaries = $ test_1_alpha1
   alpha = Real 1
End

